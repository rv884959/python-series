#data type #mutable:list dic byte array immutable:tuple string set
#number int float complex
a=4
b=4.9000
c=2+3j
print(a,type(a))
print(b,type(b))
print(c,type(c))
#string collection of value written in single or double triple quotes using multiline string
s="hello123"
print(s,type(s))
#using triple quotation
s='''hello
     welocme to my page'''
print(s,type(s))
#list written in square brackets
a=[1,3,'rahul',56]
print(a,type(a))
#tuple written in round bracket
t=(1,'rahul',44,1+6j)
print(t,type(t))
#directory unorder collection of key value pairs deine with  curly brackets
d={'name':'rahul','lastname':'verma'}
print(d,type(d))
print(d['name'])
#set nordered collection of item in witten in curly brackets
s={1,4,6,6,1}
print(s,type(s))
